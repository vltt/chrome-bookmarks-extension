{
    "name": "ToDo queue",
    "manifest_version": 2,
    "version": "0.1",
    
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "bookmarks-icon.png"
    },
    "permissions": [
        "bookmarks",
        "storage",
        "tabs"
    ]
}

